<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!--frame fill in color-->
    <solid android:color="#ffffff"/>
    <!--bolder color-->
    <stroke
        android:width="10dp"
        android:color="#64A600"/>
    <!--rectangle radius-->
    <corners android:radius="1dp"/>
    <!--padding-->
    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />
</shape>